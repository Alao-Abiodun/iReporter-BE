{
    "presets": ["@babel/preset-env"],
    "plugins": [
        ["@babel/plugin-transform-classes", {
            "loose": true
        }],
        ["@babel/plugin-transform-runtime", {
            "absoluteRuntime": true,
            "corejs": false,
            "helpers": true,
            "regenerator": true,
            "useModeules": false
        }]
    ]
}